{
  "expo": {
    "name": "CherrySpotApp",
    "slug": "CherrySpotApp",
    "version": "1.0.0",
    "sdkVersion": "52.0.19",
    "platforms": [
      "ios",
      "android"
    ],
    "jsEngine": "hermes",
    "entryPoint": "./App.js",
    "android": {
      "package": "com.maticy.CherrySpotApp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "bundleIdentifier": "com.maticy.CherrySpotApp",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {},
    "newArchEnabled": true,
    "plugins": [
      "expo-font"
    ]
  }
}
